{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft.Extensions.Http": "Warning",
        "System.Net.Http.HttpClient": "Warning"
      }
    }
  },
  "FileWatcher": {
    "WatchPath": "./data/documents"
  },
  "ApiSecurity": {
    "RequireApiKey": false
  },
  "Qdrant": {
    "VectorSize": 1024
  },
  "Rag": {
    "MinRelevanceScore": 0.2
  },
  "Embedding": {
    "Provider": "Ollama",
    "Ollama": {
      "Model": "mxbai-embed-large"
    }
  },
  "Llm": {
    "Provider": "Ollama",
    "Ollama": {
      "Model": "llama3.2:3b"
    }
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://localhost:8181"
      }
    }
  }
}
